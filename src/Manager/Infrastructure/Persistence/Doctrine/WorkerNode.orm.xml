<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Manager\Domain\Model\Dto\WorkerNode" table="worker_node">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="networkAddress" column="network_address" type="string" length="15" nullable="false" unique="false" />
        <field name="networkPort" column="network_port" type="integer" nullable="false" unique="false" />
        <field name="workerState" column="worker_state" type="string" length="32" nullable="false" unique="false" />
        <field name="joinedAt" column="joined_at" type="datetime" nullable="false" unique="false" />
        <field name="labelName" column="label_name" type="string" length="4" nullable="false" unique="false" />
        <field name="weight" column="weight" type="integer" nullable="false" unique="false" />
    </entity>

</doctrine-mapping>